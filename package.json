{
  "name": "Skullgamer205",
  "description": "My Simple WebSite",
  "version": "2025.01.19",
  "type": "module",
  "private": true,
  "bugs": {
    "url": "https://github.com/SkullGamer205/SkullGamer205/issues"
  },
  "homepage": "https://github.com/SkullGamer205/SkullGamer205/",
  "keywords": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SkullGamer205/SkullGamer205.git"
  },
  "scripts": {
    "prepublishOnly": "git push --follow-tags && gh-release",
    "test": "run-s test:*",
    "test:lint": "eslint",
    "start": "run-s watch",
    "version": "auto-changelog -p --template keepachangelog auto-changelog --breaking-pattern 'BREAKING CHANGE:' && git add CHANGELOG.md",
    "build": "npm run clean && run-p build:*",
    "build:top-bun": "tb",
    "watch": "npm run clean && run-p watch:*",
    "watch:top-bun": "npm run build:top-bun -- --watch --drafts",
    "clean": "rm -rf public && mkdir -p public",
    "new-blogpost": "node scripts/create-blogpost.js --title",
    "publish-draft": "node scripts/publish-draft.js"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "fetch-undici": "^5.0.0",
    "jsonfeed-to-atom": "^1.2.4",
    "local-storage-proxy": "^4.0.2",
    "mine.css": "^10.0.0",
    "p-map": "^7.0.2",
    "uhtml-isomorphic": "^2.0.0",
    "xmlbuilder": "^15.1.1"
  }
}
